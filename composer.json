{
  "name": "bushbaby/slmqueue-crossbarpusherstrategy",
  "description": "BsbCrossbarPusherStrategy is a strategy for the SlmQueue ZF2 module that provides the ability to publish worker events to a Crossbar.io 'pusher' endpoint",
  "type": "library",
  "keywords": [
    "zf2",
    "slm-queue",
    "module",
    "strategy",
    "crossbar.io",
    "wamp"
  ],
  "homepage": "https://github.com/bushbaby/BsbCrossbarPusherStrategy",
  "license": "MIT",
  "authors": [
    {
      "name": "Bas Kamer",
      "homepage": "http://bushbaby.nl"
    }
  ],
  "require": {
    "php": ">=5.4",
    "slm/queue": "~0.4",
    "zendframework/zend-eventmanager": "~2.0",
    "zendframework/zend-filter": "~2.0",
    "zendframework/zend-http": "~2.0",
    "zendframework/zend-json": "~2.0",
    "zendframework/zend-modulemanager": "~2.0",
    "zendframework/zend-stdlib": "~2.0",
    "zendframework/zend-uri": "~2.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "~2.0",
    "zendframework/zendframework": "~2.0",
    "phpunit/phpunit": "~4.1"
  },
  "suggests" : {
      "guzzle/http" : "Guzzle includes a cacert.pem file you might want to use for TLS connections"
  },
  "autoload": {
    "psr-4": {
      "BsbCrossbarPusherStrategy\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BsbCrossbarPusherStrategyTest\\": "test/src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}